{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@services/*": ["./src/services/*"],
      "@components/*": ["./src/components/*"],
      "@features/*": ["./src/features/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@router/*": ["./src/router/*"]
    }
  }
}
